{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@ant-design/pro-layout": "^5.0.12",
    "@ant-design/pro-table": "^2.2.8",
    "@babel/helper-compilation-targets": "^7.9.6",
    "@umijs/fabric": "^2.0.8",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.2",
    "antd": "^4.2.4",
    "axios": "^0.19.2",
    "dva": "^2.4.1",
    "lint-staged": "^10.0.7",
    "moment": "^2.26.0",
    "lodash": "^4.17.15",
    "path-to-regexp": "^6.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "router": "^1.3.5",
    "umi": "^3.2.2",
    "yorkie": "^2.0.0"
  }
}
